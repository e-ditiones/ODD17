<TEI xmlns="http://www.tei-c.org/ns/1.0">
  <teiHeader>
      <fileDesc>
         <titleStmt>
            <title>Encoding guidelines of the teiHeader for printed documents</title>
            <author>Alexandre Bartz</author>
         </titleStmt>
         <publicationStmt>
            <p>Not published yet (work still in progess)</p>
         </publicationStmt>
         <sourceDesc>
