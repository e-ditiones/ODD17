            <elementRef key="list"/>
            <elementRef key="item"/>
            <elementRef key="availability"/>
            <elementRef key="p"/>
            <elementRef key="title"/>
            <elementRef key="author"/>
            <elementRef key="bibl"/>
            <elementRef key="pubPlace"/>
            <elementRef key="respStmt"/>
            <elementRef key="resp"/>
            <elementRef key="name"/>
            <elementRef key="publisher"/>
            <elementRef key="date"/>
            <elementRef key="ptr"/>
      </schemaSpec>
      </body>
  </text>
</TEI>
